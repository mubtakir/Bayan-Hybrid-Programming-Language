def apply_inference_rule(rule_type, premises, context=None):
    """ØªØ·Ø¨ÙŠÙ‚ Ù‚Ø§Ø¹Ø¯Ø© Ø§Ø³ØªØ¯Ù„Ø§Ù„"""
    rules = {
        "deductive": apply_deductive_inference,
        "inductive": apply_inductive_inference,
        "abductive": apply_abductive_inference,
        "analogical": apply_analogical_inference
    }
    
    if rule_type not in rules:
        return {"success": False, "error": f"Unknown rule type: {rule_type}"}
    
    return rules[rule_type](premises, context)

def apply_deductive_inference(premises, context):
    """Ø§Ø³ØªØ¯Ù„Ø§Ù„ Ø§Ø³ØªÙ†ØªØ§Ø¬ÙŠ"""
    # ØªØ·Ø¨ÙŠÙ‚ Ù‚ÙˆØ§Ø¹Ø¯ Ø§Ù„Ù…Ù†Ø·Ù‚ Ø§Ù„Ø§Ø³ØªÙ†ØªØ§Ø¬ÙŠ
    conclusions = []
    for premise in premises:
        if "implies" in premise:
            # ØªØ·Ø¨ÙŠÙ‚ modus ponens
            if premise["antecedent"] in premises:
                conclusions.append(premise["consequent"])
    
    return {"success": True, "conclusions": conclusions, "confidence": 1.0}

def apply_inductive_inference(examples, context):
    """Ø§Ø³ØªØ¯Ù„Ø§Ù„ Ø§Ø³ØªÙ‚Ø±Ø§Ø¦ÙŠ"""
    # Ø§Ù„Ø¨Ø­Ø« Ø¹Ù† Ø£Ù†Ù…Ø§Ø· Ù…Ø´ØªØ±ÙƒØ©
    if not examples:
        return {"success": False, "error": "No examples provided"}
    
    # Ø§Ø³ØªØ®Ø±Ø§Ø¬ Ø§Ù„Ù†Ù…Ø· Ø§Ù„Ù…Ø´ØªØ±Ùƒ
    pattern = find_common_pattern(examples)
    confidence = len(examples) / (len(examples) + 10)  # Ø«Ù‚Ø© ØªØ²Ø¯Ø§Ø¯ Ù…Ø¹ Ø§Ù„Ø£Ù…Ø«Ù„Ø©
    
    return {"success": True, "pattern": pattern, "confidence": confidence}

def apply_abductive_inference(observation, context):
    """Ø§Ø³ØªØ¯Ù„Ø§Ù„ Ø§ÙØªØ±Ø§Ø¶ÙŠ"""
    # Ø¥ÙŠØ¬Ø§Ø¯ Ø£ÙØ¶Ù„ ØªÙØ³ÙŠØ±
    possible_explanations = generate_explanations(observation, context)
    best_explanation = max(possible_explanations, key=lambda x: x["score"])
    
    return {"success": True, "explanation": best_explanation, 
            "confidence": best_explanation["score"]}

def apply_analogical_inference(source, target, context):
    """Ø§Ø³ØªØ¯Ù„Ø§Ù„ Ù‚ÙŠØ§Ø³ÙŠ"""
    # Ø­Ø³Ø§Ø¨ Ø§Ù„ØªØ´Ø§Ø¨Ù‡
    similarity = calculate_similarity(source, target)
    
    if similarity > 0.7:
        # Ù†Ù‚Ù„ Ø§Ù„Ù…Ø¹Ø±ÙØ©
        transferred_knowledge = transfer_knowledge(source, target, similarity)
        return {"success": True, "knowledge": transferred_knowledge, 
                "confidence": similarity}
    
    return {"success": False, "error": "Insufficient similarity"}

def find_common_pattern(examples):
    """Ø¥ÙŠØ¬Ø§Ø¯ Ø§Ù„Ù†Ù…Ø· Ø§Ù„Ù…Ø´ØªØ±Ùƒ"""
    # ØªØ­Ù„ÙŠÙ„ Ø¨Ø³ÙŠØ· Ù„Ù„Ø£Ù…Ø«Ù„Ø©
    if not examples:
        return None
    
    # Ø§Ø³ØªØ®Ø±Ø§Ø¬ Ø§Ù„Ø®ØµØ§Ø¦Øµ Ø§Ù„Ù…Ø´ØªØ±ÙƒØ©
    common = {}
    for key in examples[0].keys():
        values = [ex.get(key) for ex in examples]
        if len(set(values)) == 1:
            common[key] = values[0]
    
    return common

def calculate_similarity(obj1, obj2):
    """Ø­Ø³Ø§Ø¨ Ø§Ù„ØªØ´Ø§Ø¨Ù‡ Ø¨ÙŠÙ† ÙƒØ§Ø¦Ù†ÙŠÙ†"""
    if not obj1 or not obj2:
        return 0.0
    
    common_keys = set(obj1.keys()) & set(obj2.keys())
    if not common_keys:
        return 0.0
    
    matches = sum(1 for k in common_keys if obj1[k] == obj2[k])
    return matches / len(common_keys)

